#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tortuga/Documents/test_me/node_modules/.pnpm/slug@8.2.2/node_modules/slug/node_modules:/home/tortuga/Documents/test_me/node_modules/.pnpm/slug@8.2.2/node_modules:/home/tortuga/Documents/test_me/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tortuga/Documents/test_me/node_modules/.pnpm/slug@8.2.2/node_modules/slug/node_modules:/home/tortuga/Documents/test_me/node_modules/.pnpm/slug@8.2.2/node_modules:/home/tortuga/Documents/test_me/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/slug@8.2.2/node_modules/slug/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/slug@8.2.2/node_modules/slug/cli.js" "$@"
fi
